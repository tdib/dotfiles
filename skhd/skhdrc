
# Quick apps
shift + alt - return : ./$HOME/.config/skhd/scripts/wezterm-toggle.sh
shift + alt - 1 : open -a "Arc"
shift + alt - 2 : open -a "Discord"
shift + alt - 3 : open -a "Spotify"
shift + alt - 4 : open -a "Visual Studio Code"
shift + alt - 5 : open -a ""
shift + alt - 6 : open -a ""
shift + alt - 7 : open -a ""
shift + alt - 8 : open -a ""
shift + alt - 9 : open -a ""
shift + alt - 0 : open -a "Finder"

